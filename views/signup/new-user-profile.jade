doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    meta(name='description', content='')
    meta(name='author', content='')
    title Silo
    // jQuery UI
    link(href='../../stylesheets/jquery-ui.min.css', rel='stylesheet')
    // Bootstrap Core CSS
    link(href='../../stylesheets/bootstrap.min.css', rel='stylesheet')
    link(href='../../stylesheets/bootstrap-social.css', rel='stylesheet')
    // Custom Fonts
    link(href='../../font-awesome/css/font-awesome.min.css', rel='stylesheet', type='text/css')
    link(href='https://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic', rel='stylesheet', type='text/css')
    link(href='https://fonts.googleapis.com/css?family=Montserrat:400,700', rel='stylesheet', type='text/css')
    link(href='https://fonts.googleapis.com/css?family=Source+Sans+Pro:900', rel='stylesheet', type='text/css')
    // HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries
    // WARNING: Respond.js doesn't work if you view the page via file://
    //if lt IE 9
    script(src='https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js')
    script(src='https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js')
    //underscore
    script(src="../../javascript/lib/underscore.js")
    // jQuery
    script(src='../../javascript/lib/jquery.js')
    // jQuery UI
    script(src='../../javascript/lib/jquery-ui.min.js')
    //Backbone
    script(src="../../javascript/lib/backbone.js")
    // Bootstrap Core JavaScript
    script(src='../../javascript/lib/bootstrap.min.js')
    // Plugin JavaScript
    script(src='../../javascript/lib/jquery.easing.min.js')
    link(href='../../stylesheets/new-user-profile.css', rel='stylesheet')
  body
    script#profile-template(type='text/template')
    nav.navbar.navbar-custom.navbar-fixed-top(role='navigation')
      #container.container
        .navbar-header
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-main-collapse')
            i.fa.fa-bars
          form.search_form(action='/results', method='get', name='search_form')
            button#search-dropdown.btn.btn-default
              i.fa.et-down.fa-bars
              i.fa.et-down.fa-angle-down
            input#text_search(type='text', name='tags', placeholder='Search keywords')
            button#search_button.btn.btn-info(type='submit') 
              i.fa.fa-search
        #collapse.collapse.navbar-collapse.navbar-right.navbar-main-collapse
            ul.nav.navbar-nav
              li.signin-icon
               i.fa.fa-user
              li.signin-icon
               i.fa.fa-comment-o
              li.signin-icon
               i.fa.fa-cog

  script(src= '../../javascript/new-user-profile.js')
